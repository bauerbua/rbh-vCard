{
	"name": "astro-landing-page",
	"version": "1.0.0",
	"main": "index.js",
	"watch": {
		"build": {
			"patterns": [
				"src"
			],
			"extensions": "html"
		}
	},
	"scripts": {
		"watch": "npm-watch",
		"build": "postcss styles.css -o src/styles.css",
		"dev": "concurrently \"npm run build && live-server src --port=8080\" \"npm run watch\""
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"autoprefixer": "^10.2.4",
		"layzr.js": "^2.2.2",
		"postcss-cli": "^8.3.1",
		"tailwindcss": "^3.1.8"
	},
	"devDependencies": {
		"concurrently": "^7.2.2",
		"npm-watch": "^0.11.0"
	}
}
